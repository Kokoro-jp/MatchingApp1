<h4>検索結果<%= @numbers%>件</h4>

<% @rooms.each do |room| %>
    <%= link_to controller: 'rooms', action: 'show', id: room.id do %>
        <%= image_tag room.room_img.url, :alt => 'ルーム画像', class: "frame-room-image" %>
        <p>
            <%= room.room_name %>
        </p>
        <p>
            <%= room.room_address %>
        </p>
        <p>
            <%= room.room_price %>
        </p>
    <% end %>
<% end %>
